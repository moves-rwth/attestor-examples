{
    "name":"AVL Tree search swap moving back",
    "scenario":"Verify memory safety while searching an element and swapping subtrees while moving back to root.",
    "specificationDescription":"M",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"searchAndSwap"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"AVLTreeGrammar.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"AVLTree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":5000,
        "abstractionDistance":1,
        "mode":"indexed"
    },
    "output":{
        "enabled":true,
    },
    "modelChecking":{}
}
