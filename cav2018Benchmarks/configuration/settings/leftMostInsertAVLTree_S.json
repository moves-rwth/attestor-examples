{
    "name":"AVL Tree leftmost insertion",
    "scenario":"Verify memory safety.",
    "specificationDescription":"M-S",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"leftMostInsert"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"AVLTreeGrammar.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"AVLTreeWithThis.json",
            "description":"A general balanced AVL tree with one marked element."
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":6000,
        "abstractionDistance":1,
        "mode":"indexed",
        "stateSpacePostProcessing":false,
        "garbageCollection":false
    },
    "output":{
        "enabled":false,
    },
    "modelChecking":{
        "enabled":true,
        "formulae":"(GF { terminated } -> F ({ terminated } & { btree }))"
    }
}
