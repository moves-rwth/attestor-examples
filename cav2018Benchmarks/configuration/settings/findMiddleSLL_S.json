{
    "name":"SLL find middle element",
    "scenario":"Verify that the heap is always a list.",
    "specificationDescription":"M-S",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"SLLFindMiddle",
            "method":"findMiddle"
        },
        "userDefinedGrammar":{
        },
