{
    "name":"Reverse SLL",
    "scenario":"Verify that the head reachable from tail upon termination. ",
    "specificationDescription":"M-R",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"SLLReverse",
            "method":"reverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"SLLWithTail.json"
        }
    },
    "options":{
        "abstractionDistance":1
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/RefSLL/reverseSLL_R"
        },
        "report":{
            "path":"../attestor-examples-output/RefSLL/reverseSLL_R"
        }
    },
    "modelChecking":{
       "enabled":true,
        "formulae":"(X {isReachable(head,tail,[next])} & F (X {terminated} -> {isReachable(tail,head,[next])}))"
    }
}
