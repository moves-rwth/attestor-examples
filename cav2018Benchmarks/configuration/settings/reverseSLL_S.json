{
    "name":"Reverse SLL",
    "scenario":"Verify that the result is an SLL again upon termination. ",
    "specificationDescription":"M-S",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"SLLReverse",
            "method":"reverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"SLL.json"
        }
    },
    "options":{
        "abstractionDistance":1,
        "garbageCollection":false
    },
    "output":{
        "enabled":false,
    },
    "modelChecking":{
       "enabled":true,
        "formulae":"G ({terminated} -> { L(RefSLList) })"
    }
}
