            "file":"DLL.json"
        }
    },
    "options":{
        "abstractionDistance":1,
    },
    "output":{
        "enabled":false,
    },
    "modelChecking":{
        "enabled":true,
        "formulae":"(GF {terminated} -> F {visited(pos)})"
    }
}
