{
    "name":"Reverse SLL recursively",
    "scenario":"Verify that the result is a list again.",
    "specificationDescription":"M-S",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"SLLRecursiveReverse",
            "method":"reverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"SLL.json"
        }
    },
    "options":{
        "abstractionDistance":1,
        "garbageCollection":false
    },
    "output":{
        "enabled":true,
        "stateSpace":{
            "path":"../attestor-examples-output/RefSLL/recursiveReverseSLL_M"
        }
    },
    "modelChecking":{
        "enabled":true,
        "formulae":"G ({terminated} -> { L(RefSLList) })"
    }
}
