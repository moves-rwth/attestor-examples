        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":6000,
        "abstractionDistance":1,
        "stateSpacePostProcessing":false,
        "mode":"indexed"
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/avltree/leftMostInsertAVLTree_M"
        }
    },
    "modelChecking":{}
}
