{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"TreeFlatten",
            "method":"flatten"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"dataListDataTree.json"
        },
        "predefinedGrammars":[],
        "initialState":{
            "path":"configuration/inputs",
            "file":"tree.json",
            "description":"An arbitrary binary tree with data"
        }
    },
    "options":{
        "abstractionDistance":0,
        "logging":true,
        "maximalStateSpace":100 
    },
    "output":{
        "stateSpace":{
            "path":"../attestor-examples-output/TreeFlatten/flatten"
        },
        "grammar":{
            "path":"../attestor-examples-output/TreeFlatten",
            "folder":"grammarOutput",
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"ap1, ap3 U ap4, (ap1 & ap2)"
    }        
}