{
  "name":"M-N",
  "scenario":"Analyzing the Lindtrom tree traversal for memory safety. Also prove that the successors of every element coincide with its original successors upon termination",
  "input":{
    "program":{
      "classpath":"configuration/code",
      "class":"BinaryTree",
      "method":"lindstrom"
    },
    "userDefinedGrammar":{
    },
    "predefinedGrammars":[
      { "type": "BT", "definition":"configuration/grammar/BinaryTree.java" }
    ],
    "initialState":{
      "path":"configuration/inputs",
      "file":"BinaryTree.json",
      "description":"An arbitrary binary tree"
    }
  },
  "options":{
    "abstractionDistance":0,
    "maximalStateSpace":100000
  },
  "output":{
    "enabled":false,
    "stateSpace":{
      "path":"../attestor-examples-output/BinaryTree/lindstrom_n"
    }
  },
  "modelChecking":{
    "enabled":true,
    "formulae":"( G F { terminated } -> F ( { terminated } & { identicNeighbours } ) )"
  }
}