{
    "scenario": "(S) shape: balanced tree in final states",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "AVLTree",
            "method": "searchAndBack"
        },
        "userDefinedGrammar": {
            "path": "configuration/grammar",
            "files": [
                "avltree.json"
            ]
        },
        "initialState": {
            "path": "configuration/inputs",
            "file": "balanced_tree.json",
            "description": "A general balanced AVL tree"
        },
        "predefinedGrammars": []
    },
    "options": {
        "maximalStateSpace": 2000,
        "abstractionDistance": 0,
        "mode": "indexed"
    },
    "output": {
        "enabled": false,
        "stateSpace": {
            "path": "../attestor-examples-output/avltree/binary_search_and_back_S"
        }
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "(GF { terminated } -> F ({ terminated } & { btree }))"
    }
}