--description "(S) shape: balanced tree in final states"
--classpath "configuration/code"
--class "AVLTree"
--method "minValue"
--grammar "configuration/grammar/avltree.json"
--indexed
--initial "configuration/inputs/balanced_tree.json"
--model-checking "G ({terminated} -> { btree })"
--admissible-abstraction
--no-garbage-collector
--max-state-space 500
