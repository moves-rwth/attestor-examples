{
    "name": "AVL: Rebalance",
    "specificationDescription": "M-S",
    "scenario": "Prove the tree is balanced upon termination",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "AVLTree",
            "method": "rebalance"
        },
        "userDefinedGrammar": {
            "path": "configuration/grammar",
            "files": [
                "avltree.json"
            ]
        },
        "initialState": {
            "path": "configuration/inputs",
            "file": "inserted.json",
            "description": "AVL after a node has been inserted"
        },
        "predefinedGrammars": []
    },
    "options": {
        "maximalStateSpace": 2000,
        "abstractionDistance": 1,
        "mode": "indexed"
    },
    "output": {
        "enabled": false
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "(GF { terminated } -> F ({ terminated } & { btree }))"
    }
}