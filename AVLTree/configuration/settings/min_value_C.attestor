--description "(C) correctness: return value is leftmost leaf in the tree"
--classpath "configuration/code"
--class "AVLTree"
--method "minValue"
--grammar "configuration/grammar/avltree.json"
--indexed
--initial "configuration/inputs/balanced_tree.json"
--model-checking "G ({terminated} -> { return.left == null })"
--admissible-abstraction
--max-state-space 500
