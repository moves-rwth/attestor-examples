{
    "name": "AVL: List to AVLTree",
    "specificationDescription": "M-S",
    "scenario": "Prove that the tree is balanced upon termination",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "AVLTree",
            "method": "sllToAVLTree"
        },
        "userDefinedGrammar": {
            "path": "configuration/grammar",
            "files": [
                "avltree.json"
            ]
        },
        "initialState": {},
        "predefinedGrammars": [
            {
                "type": "SLList",
                "definition": "configuration/grammar/RenameSLL.java"
            }
        ]
    },
    "options": {
        "maximalStateSpace": 6000,
        "abstractionDistance": 1,
        "stateSpacePostProcessing": false,
        "mode": "indexed",
        "garbageCollection": false
    },
    "output": {
        "enabled": true
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "(GF { terminated } -> F ({ terminated } & { btree }))"
    }
}