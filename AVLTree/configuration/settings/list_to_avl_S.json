{
    "scenario": "(S) shape: balanced tree in final states",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "AVLTree",
            "method": "sllToAVLTree"
        },
        "userDefinedGrammar": {
            "path": "configuration/grammar",
            "files": [
                "avltree.json"
            ]
        },
        "initialState": {},
        "predefinedGrammars": [
            {
                "type": "SLList",
                "definition": "configuration/grammar/RenameSLL.java"
            }
        ]
    },
    "options": {
        "maximalStateSpace": 6000,
        "abstractionDistance": 1,
        "stateSpacePostProcessing": false,
        "mode": "indexed",
        "garbageCollection": false
    },
    "output": {
        "enabled": false,
        "stateSpace": {
            "path": "../attestor-examples-output/avltree/list_to_avl_S"
        }
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "(GF { terminated } -> F ({ terminated } & { btree }))"
    }
}