{
    "name": "AVL: minimal value",
    "specificationDescription": "M-S (final)",
    "scenario": "Balanced tree upon termination",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "AVLTree",
            "method": "minValue"
        },
        "userDefinedGrammar": {
            "path": "configuration/grammar",
            "files": [
                "avltree.json"
            ]
        },
        "initialState": {
            "path": "configuration/inputs",
            "file": "balanced_tree.json",
            "description": "A general balanced AVL tree"
        },
        "predefinedGrammars": []
    },
    "options": {
        "maximalStateSpace": 500,
        "abstractionDistance": 1,
        "stateSpacePostProcessing": false,
        "mode": "indexed",
        "garbageCollection": false
    },
    "output": {
        "enabled": true
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "G ({terminated} -> { btree })"
    }
}