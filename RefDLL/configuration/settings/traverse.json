{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"DLList",
            "method":"traverse"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "DLList", "definition":"configuration/grammar/RenameDLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"dll.json",
            "description":"A heap with an arbitrary long doubly-linked list and head variable"
        }
    },
    "options":{
        "abstractionDistance":1,
        "logging":true
    },
    "output":{
        "stateSpace":{
            "path":"../attestor-examples-output/RefDLL/traverse"
        },
        "grammar":{
            "path":"../attestor-examples-output/RefDLL",
            "folder":"grammarOutput"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"ap1, ap3 U ap4, (ap1 & ap2)"
    }        
}
