{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"search"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"avltree.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"balanced_tree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":500,
        "depth":0,
        "mode":"indexed",
        "logging":false,
        "stateLabeling":["reach"],
        "stateRefinement":["balance"]
    },
    "output":{
        "defaultPath":"output",
        "stateSpace":{
            "path":".",
            "file":"stateSpace",
            "description":"State space generated for List.main()"
        }
    },
    "modelChecking":{}
}
