{
    "scenario":"memory safety",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"minValue"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "files":["avltree.json"]
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"balanced_tree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":500,
        "abstractionDistance":1,
        "stateSpacePostProcessing":false,
        "mode":"indexed",
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/avltree/min_value"
        }
    },
    "modelChecking":{
    }      
}
