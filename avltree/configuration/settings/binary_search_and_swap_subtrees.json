{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"searchAndSwitchSubtrees"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"avltree.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"balanced_tree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":5000,
        "abstractionDistance":1,
        "aggressiveAbstractionThreshold":5,
        "mode":"indexed",
        "logging":false,
        "stateRefinement":["balance"]
    },
    "output":{
        "stateSpace":{
            "path":".",
            "folder":"stateSpace"
        }
    },
    "modelChecking":{}
}
