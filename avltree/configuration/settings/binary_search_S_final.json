{
    "scenario":"(S) shape: balanced tree in final states",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"binarySearch"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"avltree.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"balanced_tree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":500,
        "abstractionDistance":0,
        "mode":"indexed",
        "logging":false,
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/avltree/binary_search"
        }
    },
    "modelChecking":{
        "enabled":true,
	    "formulae":"G ({terminated} -> { btree })"
    }
}
