{
    "scenario":"(C) correctness: return to root of the tree",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"searchAndBack"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "files":["avltree.json"]
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"balanced_tree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":2000,
        "abstractionDistance":0,
        "mode":"indexed",
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/avltree/binary_search_and_back_C"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"G ({terminated} -> { return.parent == null })"
    }
}
