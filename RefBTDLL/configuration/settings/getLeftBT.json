{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"BTNodeAlt",
            "method":"getLeft"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "BT", "definition":"configuration/grammar/RenameBT.java" },
            { "type": "DLList", "definition":"configuration/grammar/RenameDLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"bt.json",
            "description":"An arbitrary binary tree"
        }
    },
    "options":{
        "abstractionDistance":1,
        "logging":true
    },
    "output":{
        "stateSpace":{
            "path":"../attestor-examples-output/RefBTDLL/getLeftBT"
        },
        "grammar":{
            "path":"../attestor-examples-output/RefBTDLL",
            "folder":"grammarOutput",
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"ap1"
    }        
}