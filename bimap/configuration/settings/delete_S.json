{
    "scenario":"(S) shape: two doubly-linked lists of equal length",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"DLList",
            "method":"delete"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"bimap.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"bimap.json",
            "description":"A heap with an arbitrary BiMap"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":5500,
        "abstractionDistance":1,
        "mode":"indexed"
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/bimap/delete"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(GF {terminated} -> GF { bimap })"
    }
}
