{
    "scenario":"(C) correctness: element appended to last element",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"DLList",
            "method":"insert"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"bimap.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"bimap.json",
            "description":"A heap with an arbitrary BiMap"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":5500,
        "abstractionDistance":1,
        "mode":"indexed",
        "logging":false
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/bimap/insert"
        }
    },
    "modelChecking":{
        "enabled":true,
	    "formulae":"(GF { terminated } -> F ({ terminated } & { return.next == null }))"
    }
}
