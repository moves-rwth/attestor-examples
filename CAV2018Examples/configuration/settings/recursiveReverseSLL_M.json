{
    "name": "Reverse SLL recursively",
    "scenario": "Verify memory safety.",
    "specificationDescription": "M",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "SLLRecursiveReverse",
            "method": "reverseList"
        },
        "userDefinedGrammar": {},
        "predefinedGrammars": [
            {
                "type": "SLList",
                "definition": "configuration/grammar/RenameSLL.java"
            }
        ],
        "initialState": {
            "path": "configuration/inputs",
            "file": "SLL.json"
        }
    },
    "options": {
        "abstractionDistance": 1,
        "aggressiveReturnAbstraction":false
    },
    "output": {
        "enabled": true,
        "stateSpace":{ "path":"../attestor-examples-output/recursive/reverseList" },
        "contractsForInspection":{ "path":"../attestor-examples-output/recursive/reverseList" },
        "contractsForReuse":{
            "path":"configuration/contracts",
            "requestedContracts": [
                {
                    "signature": "<SLLRecursiveReverse: SLL reverseList(SLL)>",
                    "filename": "contract_reverse"
                }
            ]}
    },
    "modelChecking": {}
}