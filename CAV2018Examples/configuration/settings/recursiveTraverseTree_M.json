{
    "name": "Recursive tree traversal",
    "scenario": "Verify memory safety.",
    "specificationDescription": "M",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "RecursiveTreeTraverse",
            "method": "traverse"
        },
        "userDefinedGrammar": {},
        "predefinedGrammars": [
            {
                "type": "BT",
                "definition": "configuration/grammar/RenameBinaryTree.java"
            }
        ],
        "initialState": {
            "path": "configuration/inputs",
            "file": "BinaryTree.json",
            "description": "An arbitrary binary tree"
        }
    },
    "options": {
        "abstractionDistance": 1,
        "stateSpacePostProcessing":true,
    },
    "output": {
        "enabled": true,
        "stateSpace":{ "path":"../attestor-examples-output/recursive/treeTraversal"},
        "contractsForInspection":{ "path":"../attestor-examples-output/recursive/treeTraversal" }
    },
    "modelChecking": {}
}