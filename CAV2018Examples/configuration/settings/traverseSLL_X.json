{
    "name": "Faulty SLL traversal",
    "scenario": "Disprove that all elements are visited by variable cur upon termination.",
    "specificationDescription": "M-X",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "SLLTraverse",
            "method": "traverseFaulty"
        },
        "userDefinedGrammar": {},
        "predefinedGrammars": [
            {
                "type": "SLList",
                "definition": "configuration/grammar/RenameSLL.java"
            }
        ],
        "initialState": {
            "path": "configuration/inputs",
            "file": "SLL.json"
        }
    },
    "options": {
        "abstractionDistance": 1,
        "checkCounterexamples":true
    },
    "output": {
        "enabled": true
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "G ({terminated} -> {cur != null})"
    }
}