{
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"Test",
            "method":"main"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "BT", "definition":"configuration/grammar/RenameBT.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"bt.json",
            "description":"An arbitrary binary tree"
        }
    },
    "options":{
        "abstractionDistance":0,
        "logging":true,
        "maximalStateSpace":6000
    },
    "output":{
        "stateSpace":{
            "path":"../attestor-examples-output/RefBT/Test"
        },
        "grammar":{
            "path":"../attestor-examples-output/RefBT/Test",
            "folder":"grammarOutput"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"ap1, ap3 U ap4, (ap1 & ap2)"
    }        
}