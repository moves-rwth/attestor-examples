{
    "name": "M-N",
    "scenario": "Prove that the neighbourhood of every node is the original one upon termination.",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "Lindstrom",
            "method": "traverse"
        },
        "userDefinedGrammar": {},
        "predefinedGrammars": [
            {
                "type": "BT",
                "definition": "configuration/grammar/RenameBTLindstrom.java"
            }
        ],
        "initialState": {
            "path": "configuration/inputs",
            "file": "bt.json",
            "description": "An arbitrary binary tree"
        }
    },
    "options": {
        "abstractionDistance": 0,
        "collapseRules": false,
        "maximalStateSpace": 500000
    },
    "output": {
        "enabled": false,
        "stateSpace": {
            "path": "../attestor-examples-output/RefBT/lindstromTraverse_neighbourhood"
        }
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "( G F { terminated } -> F ( { terminated } & { identicNeighbours } ) )"
    }
}