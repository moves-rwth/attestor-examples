# Benchmark: M-N
--description "Prove that the neighbourhood of every node is the original one upon termination."
--classpath "configuration/code"
--class "Lindstrom"
--method "traverse"
--predefined-grammar "BT"
--rename BTNode=Tree left=left right=right
--initial "configuration/inputs/bt.json"
--model-checking "( G F { terminated } -> F ( { terminated } & { identicNeighbours } ) )"
--no-rule-collapsing
--max-state-space 500000
