{
    "scenario":"Prove that every node has been visited by variable cur.",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"Lindstrom",
            "method":"traverse"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "BT", "definition":"configuration/grammar/RenameBTLindstrom.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"bt.json",
            "description":"An arbitrary binary tree"
        }
    },
    "options":{
        "abstractionDistance":0,
        "maximalStateSpace":100000
    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/RefBT/lindstromTraverse_visited"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(GF {terminated} -> F { visited(cur) })"
    }        
}
