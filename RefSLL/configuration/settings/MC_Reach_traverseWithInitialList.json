{
    "name": "M-R",
    "scenario": "tail reachable from head via nextAlt selectors upon termination",
    "input": {
        "program": {
            "classpath": "configuration/code",
            "class": "ListNodeAlt",
            "method": "traverseList"
        },
        "userDefinedGrammar": {},
        "predefinedGrammars": [
            {
                "type": "SLList",
                "definition": "configuration/grammar/RenameSLL.java"
            }
        ],
        "initialState": {
            "path": "configuration/inputs",
            "file": "sllHeadTail.json"
        }
    },
    "options": {
        "abstractionDistance": 1
    },
    "output": {
        "stateSpace": {
            "enabled": true,
            "path": "../attestor-examples-output/RefSLL/MC_Reach_traverseWithInitialList"
        }
    },
    "modelChecking": {
        "enabled": true,
        "formulae": "F (X {terminated} -> {isReachable(head,tail,[nextAlt])})"
    }
}