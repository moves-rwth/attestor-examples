{
    "scenario":"SLL shape upon termination",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"ListNodeAlt",
            "method":"traverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
