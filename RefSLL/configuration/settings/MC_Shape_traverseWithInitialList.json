        "stateSpace":{
            "path":"../attestor-examples-output/RefSLL/MC_Shape_traverseWithInitialList"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(GF {terminated} -> GF { L(RefSLList) })"
    }        
}
