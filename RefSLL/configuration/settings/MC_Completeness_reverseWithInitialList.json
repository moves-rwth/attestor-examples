{
    "scenario":"all elements visited by 'current' upon termination",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"ListNodeAlt",
            "method":"reverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"sll.json"
        }
    },
    "options":{
