    },
    "output":{
        "enabled":false,
        "stateSpace":{
            "path":"../attestor-examples-output/RefSLL/MC_Completeness_reverseWithInitialList"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(GF {terminated} -> F {visited(current)})"
    }        
}
