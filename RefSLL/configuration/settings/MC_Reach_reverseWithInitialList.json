{
    "name":"M-R",
    "scenario":"tba",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"ListNodeAlt",
            "method":"reverseList"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"sllHeadTail.json"
        }
    },
    "options":{
        "abstractionDistance":1
    },
    "output":{
        "enabled":true,
        "stateSpace":{
            "path":"../attestor-examples-output/RefSLL/MC_Reach_reverseWithInitialList"
        },
        "report":{
            "path":"../attestor-examples-output/RefSLL/MC_Reach_reverseWithInitialList"
        }
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(X {isReachable(head,tail,[nextAlt])} & F (X {terminated} -> {isReachable(tail,head,[nextAlt])}))"
    }        
}
