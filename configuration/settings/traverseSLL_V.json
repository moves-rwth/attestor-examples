{
    "name":"SLL traversal",
    "scenario":"Verify that all elements are visited by variable cur upon termination.",
    "specificationDescription":"M-V",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"SLLTraverse",
            "method":"traverse"
        },
        "userDefinedGrammar":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameSLL.java" }
        ],
        "initialState":{
            "path":"configuration/inputs",
            "file":"SLL.json"
        }
    },
    "options":{
        "abstractionDistance":1
    },
    "output":{
        "enabled":true,
    },
    "modelChecking":{
       "enabled":true,
	   "formulae":"(GF {terminated} -> F {visited(cur)})"
    }        
}
