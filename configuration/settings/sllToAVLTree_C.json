{
    "name":"AVL Tree transform SLL to AVL Tree",
    "scenario":"Verify that the end of the list has been reached upon termination.",
    "specificationDescription":"M-C",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"sllToAVLTree"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"AVLTreeGrammar.json"
        },
        "initialState":{
        },
        "predefinedGrammars":[
            { "type": "SLList", "definition":"configuration/grammar/RenameList.java" }
        ],
    },
    "options":{
        "maximalStateSpace":6000,
        "abstractionDistance":1,
        "stateSpacePostProcessing":false,
        "mode":"indexed"
    },
    "output":{
        "enabled":false,
    },
    "modelChecking":{
        "enabled":true,
        "formulae":"(GF { terminated } -> F ({ terminated } & { return == null }))"
    }
}
