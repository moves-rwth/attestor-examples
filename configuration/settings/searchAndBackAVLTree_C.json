{
    "name":"AVL Tree binary search and back to root",
    "scenario":"Verify that the algorithm terminates at the root.",
    "specificationDescription":"M-C",
    "input":{
        "program":{
            "classpath":"configuration/code",
            "class":"AVLTree",
            "method":"searchAndBack"
        },
        "userDefinedGrammar":{
            "path":"configuration/grammar",
            "file":"AVLTreeGrammar.json"
        },
        "initialState":{
            "path":"configuration/inputs",
            "file":"AVLTree.json",
            "description":"A general balanced AVL tree"
        },
        "predefinedGrammars":[]
    },
    "options":{
        "maximalStateSpace":2000,
        "abstractionDistance":0,
        "mode":"indexed"
    },
    "output":{
        "enabled":false,
    },
    "modelChecking":{
        "enabled":true,
        "formulae":"G ({terminated} -> { return.parent == null })"
    }
}
